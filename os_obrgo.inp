         DEFINE TRIGGER

            NAME = key-nxtfld
            TRIGGER_TYPE = V3
            SHOW_KEY = OFF
            DESCRIPTION = 
            TEXT = <<<
            declare
               srj1 srj.srj_sifra%type;
               cursor c_srj is select srj_sifra from srj
                               where srj_sifra = :ipej.srj_sifra;
            begin
            if :ipej.srj_sifra = '%' then
                  :global.srj_sifra := :ipej.srj_sifra;
                  :ctrl.srj_sifra := :ipej.srj_sifra;
                   go_block('god');
            else
               open c_srj;
               fetch c_srj into srj1;
               if c_srj%notfound then
                  bell; message('Poslovno-ekonomska jedinica nije definisana.');
               else
                  :global.srj_sifra := :ipej.srj_sifra;
                  :ctrl.srj_sifra := :ipej.srj_sifra;
                  go_block('obr');
               end if;
               close c_srj;
            end if;
            end;
            >>>

         ENDDEFINE TRIGGER

