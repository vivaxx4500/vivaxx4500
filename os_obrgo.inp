   DEFINE PROCEDURE

      NAME = pej
      DEFINITION = <<<
      procedure pej is
         srj1 srj.srj_sifra%type;
         anal_id0 char(3);
         cursor c_srj is select srj_sifra from pis_pej
            where srj_sifra = :ipej.srj_sifra
              and upper(pis_ime) = upper(user)
              and pis_ime in (select pis_ime from pis_user
                               where upper(pis_ime) = upper(user)
                                 and pis_part = anal_id0
                                 and pis_akt = 'A')
              and exists (select srj_sifra from srj
                           where srj_sifra like pis_pej.srj_sifra);
      begin
         if upper(user) <> 'PIS' then
            anal_id0 := :global.anal_id;
            open c_srj;
            fetch c_srj into srj1;
            if c_srj%notfound then
               bell; message('PEJ nije u evidenciji.');
               raise form_trigger_failure;
            end if;
            close c_srj;
         end if;
      end;
      >>>

   ENDDEFINE PROCEDURE

   DEFINE PROCEDURE

      NAME = promena_statusa_naloga
      DEFINITION = <<<
      procedure promena_statusa_naloga is
         broj_zapisa number(4);
      begin
         broj_zapisa := 0;
         :global.ime_polja := :system.trigger_field;
         go_block('kam');
         go_record(1);
         while :system.record_status <> 'NEW' loop
            promena_statusa_stavke;
            next_record;
         broj_zapisa := broj_zapisa + 1;
         end loop;
         go_record(1);
         bell;
         message('Obracun azuriran! Broj OS - '||to_char(broj_zapisa));
      --  :prs.dazur := sysdate;
      --  :prs.razur := user;
      end;
      >>>

   ENDDEFINE PROCEDURE

   DEFINE PROCEDURE

      NAME = promena_statusa_stavke
      DEFINITION = <<<
      procedure promena_statusa_stavke is
         pom char(20);
         stopa0 number(7,3);
         rnv0 number(30,2);
         rov0 number(30,2);
         amort0 number(30,2);
         rezid0 number(30,2);
         amort1 number(30,2);
         cursor c_1 is select kam_zak, am_stopa, kam_revred, kam_ramort,
                kam_amort, nvl(kam_rezv,0) from kam
                where kam_god = :kam.kam_god
                and os_inv = :kam.os_inv
                and srj_sifra = :ipej.srj_sifra;
      begin
         stopa0 := 0;
         rnv0 := 0;
         rov0 := 0;
         amort0 := 0;
         rezid0 := 0;
         amort1 := 0;
         if :kam.kam_zak = 'N' then
          open c_1;
          fetch c_1 into pom, stopa0, rnv0, rov0, amort0, rezid0;
          if c_1%found then
          amort1 := (rnv0-rezid0)*stopa0/100;
          amort0 := amort1;
          rov0 := rov0 + amort1;
          end if;
          close c_1;
          :kam.kam_amort := amort0;
          :kam.kam_ramort := rov0;
          :kam.kam_zak := 'Z';
         elsif :kam.kam_zak <> 'N' then
             bell; message('Obracun je zakljucan!');
             raise form_trigger_failure;
         end if;
      end;
      >>>

   ENDDEFINE PROCEDURE

   DEFINE PROCEDURE

      NAME = promena_vrednosti
      DEFINITION = <<<
      procedure promena_vrednosti is
         broj_zapisa number(4);
      begin
         broj_zapisa := 0;
         :global.ime_polja := :system.trigger_field;
         go_block('kam');
         go_record(1);
         while :system.record_status <> 'NEW' loop
            promena_vrednosti_stavke;
            next_record;
         broj_zapisa := broj_zapisa + 1;
         end loop;
         go_record(1);
         bell;
         message('Azurirane vrednosti za - Broj OS - '||to_char(broj_zapisa));
      --  :prs.dazur := sysdate;
      --  :prs.razur := user;
      end;
      >>>

   ENDDEFINE PROCEDURE

   DEFINE PROCEDURE

      NAME = promena_vrednosti_stavke
      DEFINITION = <<<
      procedure promena_vrednosti_stavke is
         rezid0 number(30,2);
         rezid1 number(30,2);
         cursor c_1 is select  nvl(kam_rezv,0), nvl(kam_rrezv,0) from kam
                where kam_god = :pitanje.pitanje
                and os_inv = :kam.os_inv
                and srj_sifra = :ipej.srj_sifra;
      begin
         rezid0 := 0;
         rezid1 := 0;
         if :kam.kam_zak = 'N'
            and :kam.kam_rezv is null
            and :kam.kam_rrezv is null then
          open c_1;
          fetch c_1 into rezid0, rezid1;
          if c_1%found then
          :kam.kam_rezv := rezid0;
          :kam.kam_rrezv := rezid1;
          end if;
          close c_1;
         else
             bell; message('Rezidualne vrednosti vec postoje!');
             raise form_trigger_failure;
         end if;
      end;
      >>>

   ENDDEFINE PROCEDURE

