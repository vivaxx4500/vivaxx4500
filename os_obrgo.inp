      :ctrl.date := sysdate;
      :ctrl.forma := lower(:system.current_form);
      if :obr.kam_god is null then
         :obr.kam_god := 13.5;
      end if;
      if instr(:global.un,'@') = 0 then
         :global.un := :global.un||'/'||:global.pw;
      else
         :global.un := substr(:global.un,1,instr(:global.un,'@') - 1)||'/'||
                       :global.pw||'@'||
                       substr(:global.un,instr(:global.un,'@') + 1);
      end if;
      go_block('obr');
      >>>

